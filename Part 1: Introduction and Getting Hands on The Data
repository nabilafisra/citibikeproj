    "# Selected data from May 20, 2021 to June 30, 2021\n",
    "query = {'$select': '*',\n",
    "         '$where': 'created_date>=\"2021-05-20\" and created_date<\"2021-10-12\"', \n",
    "         '$limit': 1000000}\n",
    "\n",
    "df = pd.read_csv(API + urlencode(query), \n",
    "                 parse_dates = ['created_date'],\n",
    "                 dtype = {'incident_zip':'str'})\n",
    "df.head()"
